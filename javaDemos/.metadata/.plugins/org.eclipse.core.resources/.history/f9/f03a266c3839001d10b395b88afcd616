package miniProjectPrimeNumber;

public class main {

	public static void main(String[] args) {
		int number = 25;
		int remainder = number%2 ; 		// remainder = kalan
		int amount = 0;
		System.out.println(remainder);
		
		for(int i=2; i<=number; i++) {
			if(number%i == 0) {
				amount++;
			}
		}
		if(amount==1) {
			System.out.println(number + " is prime number");
		}

	}

}
