package datatypes;

public class main {

	public static void main(String[] args) {
		
		byte number = 17; // 1 byte yer tutar
		int number2 = 12974; // 4 byte 
		double number3 = 15.682374863; // 8 byte
		number3 = 35; // double tanımlanan değişkene integer veri atanabilir
		
		float number4 = 12; // 4 byte
		char character = 's'; // 2 byte ascii kodu olarak sayisaldir.
		boolean toTrue = true; // 1 byte

	}

}
